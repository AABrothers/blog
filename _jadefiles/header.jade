mixin nav-content()
  ul
    li: a(href="#about") About
    li: a(href="#serviced") Services
    li: a(href="#projects") Projects
    li: a(href="#contact") Contact
    
.mobile-nav
  +nav-content()

header
  .header-position
    h1.logo AABrothers-logo
    
    nav.site-nav
      +nav-content()
      
    .mobile-nav-toggle: span
